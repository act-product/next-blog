generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String   @db.Text // TEXT型に変更
  thumbnail String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
